{
  "name": "@dpack/dpack",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "dpack": "bin/dpack.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "rimraf dist && pnpm run build-bundle -w",
    "build": "rimraf dist && run-s build-bundle build-types",
    "build-bundle": "rollup --config rollup.config.ts --configPlugin typescript",
    "build-types": "run-s build-types-temp build-types-pre-patch build-types-roll build-types-post-patch build-types-check",
    "test-script": "tsx scripts/util.ts"
  },
  "dependencies": {
    "esbuild": "0.16.13",
    "resolve": "1.22.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-dynamic-import-vars": "^2.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "@types/connect": "^3.4.35",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/etag": "^1.8.1",
    "acorn": "^8.8.2",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "connect": "^3.7.0",
    "connect-history-api-fallback": "^2.0.0",
    "debug": "^4.3.4",
    "dep-types": "link:./src/types",
    "es-module-lexer": "^1.1.1",
    "etag": "^1.8.1",
    "magic-string": "^0.27.0",
    "picomatch": "^2.3.1",
    "types": "link:./types",
    "ws": "^8.12.0"
  },
  "peerDependencies": {
    "@types/node": ">= 14"
  }
}
